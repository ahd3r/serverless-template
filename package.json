{
  "name": "pp-service-template-v2",
  "version": "0.0.1",
  "description": "Serverless template v2",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest --coverage",
    "build:test": "yarn build && yarn coverage",
    "lint": "eslint . --ext js,ts --cache --fix",
    "prettier": "prettier --list-different './**/*.{js,ts}' --write",
    "deploy": "sls deploy --verbose",
    "df": "sls deploy function -f",
    "dt": "sls deploy --verbose --aws-profile tst --stage tst",
    "dtf": "sls deploy function --aws-profile tst --stage tst -f",
    "dd": "sls deploy --verbose --aws-profile dev --stage dev",
    "package": "sls package",
    "rm-versions:dry": "sls prune -n 2 --dryRun",
    "rm-versions": "sls prune -n 2"
  },
  "author": "PayPerform",
  "license": "ISC",
  "devDependencies": {
    "@payconstruct/pp-types": "*",
    "@types/aws-lambda": "^8.10.62",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "aws-sdk": "^2.751.0",
    "esbuild": "^0.14.49",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "8.0.1",
    "jest": "^28.1.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "serverless-esbuild": "^1.31.0",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-latest-layer-version": "^2.1.1",
    "serverless-offline": "^8.8.1",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.2.3",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@middy/core": "^3.1.0",
    "@middy/http-error-handler": "^3.1.0",
    "@middy/http-json-body-parser": "^3.1.0",
    "@payconstruct/pp-service-utils": "0.2.4",
    "just-group-by": "^2.0.1",
    "yaml": "^2.1.1",
    "zod": "^3.17.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  }
}
